{% extends 'admin/base.admin.html.twig' %}
{% block body %}
	<div class="page-content col-full">
		<div class="table_admin">
			<h1 class="text-center">Entit√©es de
				{{ entities.name }}</h1>
			<table>
				<thead>
					<tr>
						{% for field in entities.fields %}
							<th>{{ field }}</th>
						{% endfor %}
					</tr>
				</thead>
				<tbody>
					{% for data in entities.data %}
						<tr>
							{% for field in entities.fields %}
								<td>{{ data[field] }}</td>
							{% endfor %}
						</tr>
					{% endfor %}
				</tbody>
			</table>
		</div>
	</div>
{% endblock %}
